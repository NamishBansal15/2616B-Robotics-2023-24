    Created:      2/14/2024                                                 */\n/*    Description:  2616B Auton Code                                          */\n/*                                                                            */\n/*----------------------------------------------------------------------------*/\n\n// Include the V5 Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\nvoid auton(void)\n{\n  // Setup\n  Drivetrain.driveFor(forward, 5, inches);\n  Drivetrain.turnFor(left, 90, degrees);\n  Drivetrain.driveFor(forward, 2, inches);\n  Drivetrain.turnFor(right, 90, degrees);\n  Drivetrain.driveFor(forward, 3, inches);\n  Drivetrain.turnFor(right, 90, degrees);\n\n  // Defining an intake turning variable\n  bool i = false;\n\n  // Scoring alliance triball\n  i = true;\n  Drivetrain.driveFor(forward, 0.5, inches);\n  while(i)\n  {\n    Intake.spin(reverse); // Triball out\n  }\n  wait(500, msec);\n  i = false;\n  Drivetrain.driveFor(reverse, 0.5, inches);\n  Drivetrain.turnFor(left, 180, degrees);\n\n  // Scoring 2nd triball\n  i = true;\n  while(i)\n  {\n    Intake.spin(forward); // Triball in\n  }\n  Drivetrain.driveFor(forward, 4, inches);\n  wait(100, msec);\n  Drivetrain.turnFor(left, 180, degrees);\n  Drivetrain.driveFor(forward, 4.5, inches);\n  i = false;\n  wait(100, msec);\n  while(!i)\n  {\n    Intake.spin(reverse); // Triball out\n  }\n  wait(500, msec);\n}\n\nint main() {\n  competition Competition1;\n\n  Competition1.autonomous(auton);\n\n  while(true)\n  {\n    wait(100, msec);\n  }\n}\n","textLanguage":"cpp","rconfig":[{"port":[3,4,1,2,0],"name":"Drivetrain","customName":false,"deviceType":"Drivetrain","deviceClass":"smartdrive","setting":{"type":"4-motor","wheelSize":"wheel4in","gear":"ratio6_1","gearRatio":"36:84","direction":"fwd","gyroType":"none","width":"6","unit":"in","wheelbase":"5","wheelbaseUnit":"in","xOffset":"0","xOffsetUnit":"mm","yOffset":"0","yOffsetUnit":"mm","theta